#include <iostream>

using namespace std;
int main()
{
    int i,n,s=1,j;
    cin>>n;
    int p[n+1],a[n+1];
    for(i=1;i<=n;i++){
        cin>>p[i];
    }
    for(i=1;i<=n;i++){
        for(j=1;j<=n;j++){
            if(p[j]==i){
                a[i]=j;
            }
        }
    }
    for(i=1;i<=n;i++){
        cout<<a[i]<<" ";
    }


    return 0;
}
